<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/global_whitecolor"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="28dp"
        android:layout_marginRight="28dp"
        android:layout_marginTop="18dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:background="@drawable/dialog_project_calculator_topbg"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_margin="10dp"
                android:text="承接金额"
                android:textColor="@color/bg_one_black"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/tvMoney"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="6dp"
                android:maxLength="7"
                android:maxLines="1"
                android:textColor="@color/text_red_dark"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:background="@drawable/dialog_project_calculator_topbg"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_margin="10dp"
                android:text="预计总收益"
                android:textColor="@color/bg_one_black"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/tvIncome"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="6dp"
                android:maxLines="1"
                android:textColor="@color/text_red_dark"
                android:textSize="18sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="28dp"
        android:layout_marginRight="28dp"
        android:layout_marginTop="12dp">

        <TextView
            android:id="@+id/tv1"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:text="1"
            android:textColor="@color/bg_one_black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tv2"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:text="2"
            android:textColor="@color/bg_one_black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tv3"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:text="3"
            android:textColor="@color/bg_one_black"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="28dp"
        android:layout_marginRight="28dp"
        android:layout_marginTop="10dp">

        <TextView
            android:id="@+id/tv4"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:text="4"
            android:textColor="@color/bg_one_black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tv5"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:text="5"
            android:textColor="@color/bg_one_black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tv6"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:text="6"
            android:textColor="@color/bg_one_black"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="28dp"
        android:layout_marginRight="28dp"
        android:layout_marginTop="10dp">

        <TextView
            android:id="@+id/tv7"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:text="7"
            android:textColor="@color/bg_one_black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tv8"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:text="8"
            android:textColor="@color/bg_one_black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tv9"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:text="9"
            android:textColor="@color/bg_one_black"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="28dp"
        android:layout_marginRight="28dp"
        android:layout_marginTop="10dp">

        <TextView
            android:id="@+id/tvClearAll"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:text="清除"
            android:textColor="@color/bg_one_black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tv0"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:text="0"
            android:textColor="@color/bg_one_black"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/ivClearOne"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:background="@drawable/dialog_project_calculator_numbg"
            android:gravity="center"
            android:scaleType="center"
            android:src="@drawable/dialog_project_calculator_clearone" />
    </LinearLayout>

    <ImageView
        android:id="@+id/ivClose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="4dp"
        android:padding="23dp"
        android:src="@drawable/project_calculator_down" />
</LinearLayout>